{
    "liveServer.settings.root"  : "source/frontend",
    "liveServer.settings.mount" : [
        [ "movies", "source/backend" ]
    ],

    "liveServer.settings.proxy": {
        "enable"   : true,                             //set it true to enable the feature.
        "baseUri"  : "/backend/movies",                 //from where you want to proxy.
        "proxyUri" : "http://github.com/mauricio-lima/sky-frontend/tree/gh-pages/backend/movies"   //the actual url.
    }

}
